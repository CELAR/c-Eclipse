<?xml version="1.0" encoding="UTF-8"?>
<Definitions id="CELARPolicyTypes" name="CELARPolicyTypes"
  targetNamespace="http://www.example.com/Types/CELARPolicyTypes"
  xmlns="http://docs.oasis-open.org/tosca/ns/2011/12"
  xmlns:eprop="http://www.example.org/PolicyProperties">
  
  <Import importType="http://www.w3.org/2001/XMLSchema"
    namespace="http://www.example.org/PolicyProperties"/>
    
  <PolicyType name="ElasticityPolicy">
  	<documentation> Elasticity Policy </documentation>
  </PolicyType>
  
  <PolicyType name="ElasticityConstraint">
  	<documentation> Elasticity Constraint </documentation>
  	<DerivedFrom typeRef="ElasticityPolicy"/>
  	<PropertiesDefinition element="eprop:ElasticityConstraintProperties"/>
  </PolicyType>
  
  <PolicyType name="ElasticityStrategy">
  	<documentation> Elasticity Strategy </documentation>
  	<DerivedFrom typeRef="ElasticityPolicy"/>
  	<PropertiesDefinition element="eprop:ElasticityStrategyProperties"/>
  </PolicyType>
  
 </Definitions>