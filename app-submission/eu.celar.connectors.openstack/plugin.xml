<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         name="OpenStack Element Creators"
         point="eu.celar.core.cloudElementCreator">
      <creator
            class="eu.celar.connectors.openstack.OpenStackCloudProviderCreator"
            id="eu.celar.connectors.openstack.openstackCloudProviderCreator"
            name="OpenStack Cloud Provider"
            wizard="eu.celar.connectors.openstack.ui.wizards.openstackCloudProviderWizard">
         <source
               class="org.eclipse.core.filesystem.IFileStore"
               matcher="eu.celar.connectors.openstack.OpenStackCloudProviderCreator">
         </source>
         <target
               class="eu.celar.connectors.openstack.OpenStackCloudProvider"
               name="OpenStack">
         </target>
      </creator>
      <creator
            class="eu.celar.connectors.openstack.OpenStackDeploymentServiceCreator"
            id="eu.celar.connectors.openstack.OpenStackDeploymentServiceCreator"
            name="OpenStack Application Deployment Service">
         <target
               class="eu.celar.connectors.openstack.OpenStackApplicationDeploymentService"
               name="OpenStack Deployment Service">
         </target>
         <source
               class="java.net.URI">
         </source>
         <source
               class="org.eclipse.core.filesystem.IFileStore">
         </source>
      </creator>
   </extension>
   <extension
         point="eu.celar.core.authTokens">
      <token
            description="eu.celar.connectors.openstack.auth.OpenStackAuthTokenDescription"
            id="eu.celar.connectors.openstack.authToken"
            name="OpenStack Credentials"
            wizard="eu.celar.connectors.openstack.ui.wizards.openstackAuthTokenWizard">
      </token>
   </extension>
</plugin>
