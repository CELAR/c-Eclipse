<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="eu.celar.ui.newCloudProviderWizards">
      <wizard
            class="eu.celar.connectors.aws.ui.wizards.AWSCloudProviderWizard"
            id="eu.celar.connectors.aws.ui.wizards.awsCloudProviderWizard"
            name="AWS Cloud Provider">
      </wizard>
   </extension>
<extension
         point="eu.celar.ui.authTokenUI">
      <wizard
            class="eu.celar.connectors.aws.ui.wizards.AWSAuthTokenWizard"
            icon="icons/obj16/proxy.gif"
            id="eu.celar.connectors.aws.ui.wizards.awsAuthTokenWizard"
            name="AWS Auth Token">
      </wizard>
      <factory
            class="eu.celar.connectors.aws.ui.dialogs.AWSAuthTokenUIFactory"
            id="eu.celar.connectors.aws.ui.awsAuthTokenUIFactory"
            name="AWS Auth Token UI Factory">
      </factory>
